{
  "name": "shepherd-cli",
  "version": "0.0.15",
  "description": "A framework for building APIs with AWS API Gateway and Lambda",
  "main": "lib/index.js",
  "scripts": {
    "compile": "./node_modules/.bin/babel -d lib/ src/",
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register",
    "lint": "jshint --exclude ./node_modules .",
    "prepublish": "npm run compile"
  },
  "keywords": [
    "serverless",
    "serverless framework",
    "api gateway",
    "lambda",
    "aws",
    "aws lambda",
    "amazon",
    "amazon web services",
    "shepherd",
    "lambda framework"
  ],
  "author": "Steve Faulkner <steve@bustle.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/bustlelabs/shepherd"
  },
  "preferGlobal": true,
  "bin": {
    "shepherd": "lib/cli.js"
  },
  "dependencies": {
    "aws-sdk": "southpolesteve/aws-sdk-js#patch-1",
    "babel-core": "^6.4.5",
    "babel-register": "^6.5.2",
    "bluebird": "^2.10.0",
    "fs-extra": "^0.26.4",
    "fs-extra-promise": "^0.3.1",
    "glob": "^7.0.0",
    "inquirer": "^0.11.3",
    "lodash": "^3.10.1",
    "meow": "^3.7.0",
    "pkg-conf": "^1.1.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^3.2.0",
    "expect.js": "^0.3.1",
    "mocha": "^2.3.4",
    "mockery": "bustlelabs/mockery",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
